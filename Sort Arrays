#include<stdio.h>

void main(){
int n=10,temp;
int arr[10];
for(int i=0;i<n;i++){
printf("Please Enter the Nums%d:",i+1);
scanf("%d",&arr[i]);
}
for(int j=0;j<9;j++){
for(int p=0;p<9;p++){
if(arr[p]<arr[p+1]){
temp=arr[p];
arr[p]=arr[p+1];
arr[p+1]=temp;
}
}
}
for(int c=0;c<10;c++)  {
printf(" %d   ",arr[c]);
}
scanf("%d",&temp);
}
